function CheckGame () { 
if ( linu2._alpha == linu2Al  & linu3._alpha == linu3Al &
	linu4._alpha == linu4Al & linu5._alpha == linu5Al &
	linu6._alpha == linu6Al & linu7._alpha == linu7Al &
	linu9._alpha == linu9Al & linu10._alpha == linu10Al &
	linu11._alpha == linu11Al & linu12._alpha == linu12Al &
	linu14._alpha == linu14Al &
	linu15._alpha == linu15Al & linu16._alpha == linu16Al &
	linu17._alpha == linu17Al & linu18._alpha == linu18Al &
	linu19._alpha == linu19Al & linu20._alpha == linu20Al &
	linu21._alpha == linu21Al & linu22._alpha == linu22Al &
	linu23._alpha == linu23Al & linu24._alpha == linu24Al &
	linu25._alpha == linu25Al & linu26._alpha == linu26Al &
	linu27._alpha == linu27Al & linu28._alpha == linu28Al &
	linu29._alpha == linu29Al & linu30._alpha == linu30Al &
	linu31._alpha == linu31Al & linu32._alpha == linu32Al &
	linu33._alpha == linu33Al & linu34._alpha == linu34Al &
	linu35._alpha == linu35Al & linu36._alpha == linu36Al &
	linu37._alpha == linu37Al & linu38._alpha == linu38Al &
	linu39._alpha == linu39Al & linu40._alpha == linu40Al &
	linu41._alpha == linu41Al) {
	gegel.play();
	gegel._x = 298.95;
	gegel._y = 89.85;
	touch.play();
   } 
}

gegel.onRelease=function () {
sucs.play("forward");
abt.play("forw");
touch._alpha = 0;
}



abt.onRelease=function(){ 
inf.play(); 
bck.play();
} 

bck.onRelease=function(){ 
inf.play("backward"); 
bck.play("back"); 
sucs.play("nazad"); 
abt.play();
gegel._x=-500;
gegel._y=90;
}

